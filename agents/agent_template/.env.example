# Agent Template Environment Configuration
# Copy this file to .env in the same directory and fill in your credentials

# API credentials for the service this agent interacts with
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# Other agent-specific settings
CUSTOM_SETTING_1=default_value
CUSTOM_SETTING_2=default_value

# The agent's behavior can be fine-tuned with the following settings
AGENT_MODE=standard  # standard, aggressive, conservative
